{"ast":null,"code":"import { mapActions } from 'vuex';\nimport api from '@/services/api';\nexport default {\n  name: 'LoginView',\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: null,\n      loading: false\n    };\n  },\n  methods: {\n    ...mapActions(['login']),\n    async handleLogin() {\n      this.loading = true;\n      this.error = null;\n      try {\n        await this.login({\n          email: this.email,\n          password: this.password\n        });\n        this.$router.push('/');\n      } catch (error) {\n        this.error = error.response?.data?.message || 'Login failed. Please try again.';\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapActions","api","name","data","email","password","error","loading","methods","handleLogin","login","$router","push","response","message"],"sources":["C:\\Users\\HP\\Documents\\mooban4\\frontend\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"login\">\r\n    <h1>Login</h1>\r\n    <div class=\"form-container\">\r\n      <form @submit.prevent=\"handleLogin\">\r\n        <div class=\"form-group\">\r\n          <label for=\"email\">Email</label>\r\n          <input\r\n            type=\"email\"\r\n            id=\"email\"\r\n            v-model=\"email\"\r\n            required\r\n            placeholder=\"Enter your email\"\r\n          >\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <label for=\"password\">Password</label>\r\n          <input\r\n            type=\"password\"\r\n            id=\"password\"\r\n            v-model=\"password\"\r\n            required\r\n            placeholder=\"Enter your password\"\r\n          >\r\n        </div>\r\n\r\n        <button type=\"submit\" :disabled=\"loading\">\r\n          {{ loading ? 'Logging in...' : 'Login' }}\r\n        </button>\r\n\r\n        <p v-if=\"error\" class=\"error-message\">{{ error }}</p>\r\n      </form>\r\n\r\n      <p>\r\n        Don't have an account?\r\n        <router-link to=\"/register\" class=\"link\">Register here</router-link>\r\n      </p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions } from 'vuex'\r\nimport api from '@/services/api'\r\n\r\nexport default {\r\n  name: 'LoginView',\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      error: null,\r\n      loading: false\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions(['login']),\r\n    async handleLogin() {\r\n      this.loading = true\r\n      this.error = null\r\n\r\n      try {\r\n        await this.login({\r\n          email: this.email,\r\n          password: this.password\r\n        })\r\n        this.$router.push('/')\r\n      } catch (error) {\r\n        this.error = error.response?.data?.message || 'Login failed. Please try again.'\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AA2CA,SAASA,UAAS,QAAS,MAAK;AAChC,OAAOC,GAAE,MAAO,gBAAe;AAE/B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAO,EAAE;IACP,GAAGR,UAAU,CAAC,CAAC,OAAO,CAAC,CAAC;IACxB,MAAMS,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACF,OAAM,GAAI,IAAG;MAClB,IAAI,CAACD,KAAI,GAAI,IAAG;MAEhB,IAAI;QACF,MAAM,IAAI,CAACI,KAAK,CAAC;UACfN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC;QACD,IAAI,CAACM,OAAO,CAACC,IAAI,CAAC,GAAG;MACvB,EAAE,OAAON,KAAK,EAAE;QACd,IAAI,CAACA,KAAI,GAAIA,KAAK,CAACO,QAAQ,EAAEV,IAAI,EAAEW,OAAM,IAAK,iCAAgC;MAChF,UAAU;QACR,IAAI,CAACP,OAAM,GAAI,KAAI;MACrB;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}